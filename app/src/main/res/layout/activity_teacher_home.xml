<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FAFAFA"
    tools:context=".TeacherHome">

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/yonteac" />

    <ScrollView
        android:id="@+id/scrollView4"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#D2FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:text="@string/studAtt"
                android:textColor="#000000"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="@+id/btnacccc"
                app:layout_constraintStart_toStartOf="@+id/btnacccc"
                app:layout_constraintTop_toBottomOf="@+id/btnacccc" />

            <Button
                android:id="@+id/student"
                android:layout_width="96dp"
                android:layout_height="108dp"
                android:layout_marginTop="24dp"
                android:layout_marginEnd="64dp"
                android:backgroundTint="#E5E5FF"
                android:scaleType="fitXY"
                app:cornerRadius="10dp"
                app:icon="@drawable/stt"
                app:iconTint="#0000FF"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toEndOf="@+id/button3"
                app:layout_constraintTop_toBottomOf="@+id/textView18"
                tools:ignore="SpeakableTextPresentCheck" />

            <Button
                android:id="@+id/btnacccc"
                android:layout_width="96dp"
                android:layout_height="108dp"
                android:layout_marginStart="64dp"
                android:layout_marginTop="32dp"
                android:backgroundTint="#E5E5FF"
                android:scaleType="fitXY"
                app:cornerRadius="10dp"
                app:icon="@drawable/atendico"
                app:iconTint="#0000FF"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView20"
                tools:ignore="SpeakableTextPresentCheck" />

            <ImageView
                android:id="@+id/imageView20"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:cropToPadding="false"
                android:scaleType="fitXY"
                android:src="@drawable/eeetop"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageView24"
                android:layout_width="142dp"
                android:layout_height="133dp"
                android:layout_marginTop="28dp"
                android:src="@drawable/eelogo"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.498"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/imageView20"
                app:srcCompat="@drawable/component_20" />

            <Button
                android:id="@+id/button3"
                android:layout_width="96dp"
                android:layout_height="108dp"
                android:layout_marginStart="66dp"
                android:layout_marginTop="24dp"
                android:backgroundTint="#E5E5FF"
                android:scaleType="fitXY"
                app:cornerRadius="10dp"
                app:icon="@drawable/classs"
                app:iconTint="#0000FF"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView13"
                tools:ignore="SpeakableTextPresentCheck" />

            <TextView
                android:id="@+id/textView19"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:text="@string/Students"
                android:textColor="#000000"
                app:layout_constraintEnd_toEndOf="@+id/student"
                app:layout_constraintStart_toStartOf="@+id/student"
                app:layout_constraintTop_toBottomOf="@+id/student" />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:text="@string/Teacher"
                android:textColor="#000000"
                android:textSize="24sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/imageView24"
                app:layout_constraintStart_toStartOf="@+id/imageView24"
                app:layout_constraintTop_toBottomOf="@+id/imageView24" />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:text="@string/examRes"
                android:textColor="#000000"
                app:layout_constraintEnd_toEndOf="@+id/btnexam"
                app:layout_constraintStart_toStartOf="@+id/btnexam"
                app:layout_constraintTop_toBottomOf="@+id/btnexam" />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="59dp"
                android:layout_height="20dp"
                android:layout_marginTop="5dp"
                android:text="@string/Claes"
                android:textColor="#000000"
                app:layout_constraintEnd_toEndOf="@+id/button3"
                app:layout_constraintStart_toStartOf="@+id/button3"
                app:layout_constraintTop_toBottomOf="@+id/button3" />

            <Button
                android:id="@+id/btnexam"
                android:layout_width="96dp"
                android:layout_height="108dp"
                android:layout_marginTop="32dp"
                android:layout_marginEnd="64dp"
                android:backgroundTint="#E5E5FF"
                android:scaleType="fitXY"
                app:cornerRadius="10dp"
                app:icon="@drawable/exam"
                app:iconTint="#0000FF"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toEndOf="@+id/btnacccc"
                app:layout_constraintTop_toBottomOf="@+id/imageView20"
                tools:ignore="SpeakableTextPresentCheck" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>